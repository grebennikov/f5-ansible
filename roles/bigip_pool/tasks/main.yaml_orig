---

- name: Create pool
  bigip_pool:
      lb_method: "{{ pool_lb_method }}"
      name: "{{ pool_name }}"
      password: "{{ bigip_password }}"
      server: "{{ inventory_hostname }}"
      server_port: "{{ bigip_port }}"
      state: "present"
      user: "{{ bigip_username }}"
      validate_certs: "{{ validate_certs }}"
  register: result

- name: Assert Create pool
  assert:
      that:
          - result|changed

